<IfModule mod_headers.c>
    <FilesMatch "\.(ico|gif|jpg|jpeg|png|webp|svg|woff|woff2)$">
          Header set Cache-Control "max-age=604800"
    </FilesMatch>
    <FilesMatch "\.(css|js)$">
          Header set Cache-Control "max-age=36000"
    </FilesMatch>
</IfModule>

ErrorDocument 404 /404.html
